#definition of dimensions and metrics
:availability
    a dqv:Dimension ;
    skos:prefLabel "Availability"@en ;
    skos:definition "Availability of a dataset is the extent to which data (or some portion of it) is present, obtainable and ready for use."@en ; 
    dqv:inCategory :accessibility
    .

:licensing
    a dqv:Dimension ;
    skos:prefLabel "Licensing"@en ;
    skos:definition "Metric related to the license of the dataset."@en ; 
    dqv:inCategory :accessibility
    .

:interlinking
    a dqv:Dimension ;
    skos:prefLabel "Interlinking"@en ;
    skos:definition "Metric related to the degree of connection of the KG"@en ; 
    dqv:inCategory :accessibility
    .

:security
    a dqv:Dimension ;
    skos:prefLabel "Security"@en ;
    skos:definition "Metric related to the security of the dataset."@en ; 
    dqv:inCategory :accessibility
    .

:accuracy
    a dqv:Dimension ;
    skos:prefLabel "Accuracy"@en ;
    skos:definition "Metric related to the KG's semantic accuracy"@en ; 
    dqv:inCategory :intrinsic
    .

:believability
    a dqv:Dimension ;
    skos:prefLabel "Believability"@en ;
    skos:definition "Metric related to the provenance of the KG"@en ; 
    dqv:inCategory :accessibility
    .

:verifiability
    a dqv:Dimension ;
    skos:prefLabel "Verifiability"@en ;
    skos:definition "Metric related to the autenticity of the dataset."@en ; 
    dqv:inCategory :accessibility
    .

:representational-conciseness
    a dqv:Dimension ;
    skos:prefLabel "Presentational-conciseness"@en ;
    skos:definition "Metric related to the conciseness of the dataset."@en ; 
    dqv:inCategory :accessibility
    .

:interpretability
    a dqv:Dimension ;
    skos:prefLabel "Interpretability"@en ;
    skos:definition "Metric related to the interpretability of the dataset."@en ; 
    dqv:inCategory :accessibility
    .

:sparqlEndpoint
    a dqv:Metric ;
    skos:definition "It checks if the SPARQL endpoint is online."@en ;
    dqv:expectedDataType xsd:string ;
    dqv:inDimension :availability
    .

:RDFDumpM
    a dqv:Metric ;
